{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "moduleNameMapper": {
    "^~(.*)$": "<rootDir>/src/$1"
  },
  "testRegex": ".*\\.(spec|e2e-spec|test)\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "@swc/jest"
  },
  "collectCoverageFrom": [
    "**/*.(t|j)s"
  ],
  "coverageDirectory": "../coverage",
  "testEnvironment": "node"
}
