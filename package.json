{
  "name": "@hodfords/api-gateway",
  "version": "0.0.2",
  "description": "The API Gateway houses the source code and documentation for the API Gateway - a powerful and versatile solution for managing and deploying APIs within a distributed and microservices-oriented architecture.",
  "author": "",
  "private": false,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hodfords-solutions/nestjs-api-gateway.git"
  },
  "bugs": {
    "url": "https://github.com/hodfords-solutions/nestjs-api-gateway/issues"
  },
  "homepage": "https://opensource.hodfords.uk/",
  "scripts": {
    "prebuild": "rimraf dist",
    "postbuild": "cp package.json dist/lib && cp README.md dist/lib",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"lib/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "devDependencies": {
    "@hodfords/nestjs-eslint-config": "^10.0.0",
    "@nestjs/axios": "3.0.3",
    "@nestjs/common": "10.4.3",
    "@nestjs/core": "10.4.3",
    "@nestjs/platform-express": "10.4.3",
    "@nestjs/schedule": "4.1.1",
    "@nestjs/serve-static": "4.0.2",
    "@nestjs/swagger": "7.4.2",
    "@songkeys/nestjs-redis": "10.0.0",
    "@squarecloud/http-proxy": "1.2.0",
    "axios": "1.7.7",
    "camelcase-keys": "7.0.2",
    "dotenv": "16.4.5",
    "hbs": "4.2.0",
    "ioredis": "5.4.1",
    "lodash": "4.17.21",
    "path-to-regexp": "8.1.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.1",
    "@nestjs/cli": "10.4.5",
    "@nestjs/schematics": "10.1.4",
    "@nestjs/testing": "10.4.3",
    "@types/cron": "2.4.0",
    "@types/express": "4.17.21",
    "@types/http-proxy": "1.17.15",
    "@types/lodash": "4.17.7",
    "@types/node": "22.5.5",
    "eslint": "9.11.0",
    "prettier": "3.3.3",
    "source-map-support": "0.5.21",
    "ts-loader": "9.5.1",
    "typescript": "5.6.2"
  }
}
